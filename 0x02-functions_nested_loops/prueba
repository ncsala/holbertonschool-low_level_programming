#include <stdio.h>
#include "holberton.h"

/**
 * print_last_digit: prints the last digit of a number.
 * @n: 
 * @r:
 *
 * Returns: value of the last digit.
 */

int main(void)
{
	print_last_digit(-163);
	putchar('\n');
	return (0);

}


int print_last_digit(int n)
{
	if (n < 0)
		n = n * -1;

	_putchar((n % 10) + '0');
	
	return (n % 10);	
}

